{
  "manifest_version": 2,
  "name": "test",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"],
		"persistent": false
  },

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.4.0.min.js","content.js"]
    }
  ],

  "browser_action": {
    "popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "https://sis.nyu.edu/psc/csprod/EMPLOYEE/SA/c/NYU_SR.NYU_CLS_SRCH.GBL",
    "tabs",
    "storage"
  ]

}
